.cdx-menu-item[title*="/"]:not([title$="/"]) {
    display: none !important;
}
#mw-content-text {
    margin-top: 0 !important;
}

.docnav {
  background-color: var(--background-color-neutral, #eaecf0);
  border: 1px solid var(--border-color-base, #a2a9b1);
  padding: 2px 10px;
  margin: 10px 0;
  line-height: 1;
  clear: both;
  overflow: hidden; 
  /*border-radius: 4px;*/

}

.docnav > div:first-child {
  margin: 4px 0;
  float: left;
}

.docnav > div:nth-child(2) {
  margin: 4px 0;
  float: right;
}

.docnav > div:last-child {
  text-align: center;
}

.docnav > div:last-child > div {
  margin: 4px 0;
}

.docnav img {
  vertical-align: middle;
}

.UnfinishedDocu {
  text-align: center;
  background: var(--background-color-warning-subtle, #FDE0A8);
  margin: 1em 7em;
  padding: 0.5em 2em;
  border: 2px solid var(--border-color-warning, #FF5706);
}